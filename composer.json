{
	"name": "aipng/json-serializer",
	"type": "library",
	"description": "json serializer (jms serializer + custom data handlers)",
	"license": "MIT",
	"authors": [
		{
			"name": "Karel HorskÃ½",
			"email": "karel.horsky@centrum.cz"
		}
	],
	"autoload": {
		"psr-4": {
			"AipNg\\JsonSerializer\\": "src/"
		},
		"classmap": [
			"src/Exception"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"AipNg\\JsonSerializerTests\\": "tests/src/"
		},
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"config": {
		"sort-packages": true
	},
	"require": {
		"php": ">= 7.2",
		"doctrine/cache": "^1.7",
		"jms/serializer": "^2.0"
	},
	"require-dev": {
		"aipng/value-objects": "^1.0",
		"mikey179/vfsStream": "^1.6",
		"phpstan/phpstan": "^0.10",
		"phpstan/phpstan-phpunit": "^0.10",
		"phpunit/phpunit": "^7.2"
	},
	"suggest": {
		"aipng/value-objects": "For custom serialization of AipNg value objects",
		"nette/di": "To serializer service with a Nette DI extension"
	}
}
