{
	"name": "aipng/json-serializer",
	"type": "library",
	"description": "json serializer (jms serializer + custom data handlers)",
	"license": "MIT",
	"authors": [
		{
			"name": "Karel HorskÃ½",
			"email": "karel.horsky@centrum.cz"
		}
	],
	"autoload": {
		"psr-4": {
			"AipNg\\JsonSerializer\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"AipNg\\JsonSerializerTests\\": "tests/src/",
			"AipNg\\JsonSerializerTests\\Integration\\": "tests/integration"
		}
	},
	"config": {
		"sort-packages": true
	},
	"require": {
		"php": ">= 8.3",
		"jms/serializer": "^3.31.1",
		"symfony/cache": "^v7.1"
	},
	"require-dev": {
		"aipng/value-objects": "^3.1",
		"mikey179/vfsstream": "^1.6",
		"nette/di": "^v3.0",
		"phpstan/phpstan": "^2.0.3",
		"phpstan/phpstan-nette": "^2.0",
		"phpstan/phpstan-phpunit": "^2.0.1",
		"phpunit/phpunit": "^11.4.2",
		"symfony/validator": "^7.2"
	},
	"suggest": {
		"aipng/value-objects": "Custom serialization of AipNg value objects",
		"contributte/validator": "To use symfony/validator adapter with a Nette DI extension",
		"nette/di": "Register serializer in Nette DI container"
	}
}
