includes:
	- vendor/phpstan/phpstan-phpunit/extension.neon
	- vendor/phpstan/phpstan-phpunit/rules.neon
	- vendor/phpstan/phpstan-nette/rules.neon


parameters:
	level: 8

	paths:
		- %rootDir%/../../../src
		- %rootDir%/../../../tests


	fileExtensions:
